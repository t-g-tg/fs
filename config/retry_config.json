{
  "retry_configurations": {
    "network_operations": {
      "max_retries": 3,
      "base_delay": 2.0,
      "max_delay": 30.0,
      "exponential_factor": 2.0,
      "jitter": true,
      "timeout_multiplier": 1.5
    },
    "api_operations": {
      "max_retries": 5,
      "base_delay": 3.0,
      "max_delay": 60.0,
      "exponential_factor": 2.0,
      "jitter": true,
      "timeout_multiplier": 2.0
    },
    "database_operations": {
      "max_retries": 3,
      "base_delay": 1.0,
      "max_delay": 20.0,
      "exponential_factor": 1.8,
      "jitter": true,
      "timeout_multiplier": 1.3
    },
    "form_analysis": {
      "max_retries": 2,
      "base_delay": 5.0,
      "max_delay": 45.0,
      "exponential_factor": 2.5,
      "jitter": true,
      "timeout_multiplier": 2.0
    }
  },
  "circuit_breaker": {
    "failure_threshold": 5,
    "recovery_timeout_seconds": 300,
    "min_request_threshold": 10,
    "enabled": true
  },
  "timeout_settings": {
    "form_page_load": 30000,
    "groq_api_request": 60000,
    "supabase_operation": 20000,
    "browser_initialization": 45000
  },
  "error_handling": {
    "log_retry_attempts": true,
    "log_error_details": true,
    "include_stack_trace": false,
    "alert_on_permanent_failure": true,
    "max_error_context_length": 500
  },
  "performance_tuning": {
    "enable_jitter": true,
    "adaptive_timeout": false,
    "backoff_strategy": "exponential",
    "parallel_request_limit": 3
  }
}